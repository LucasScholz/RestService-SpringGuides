pipeline {
    agent {
        docker {
            image 'maven:3-alpine' 
            args '-v /root/.m2:/root/.m2' 
        }
    }
    stages {
        stage('Build') { 
            steps {
                sh "pwd"
                sh "ls -la"
                sh "cd complete"
                sh "pwd"
                sh "ls -la"
                sh 'mvn clean package' 
            }
        }
    }
}
